Host gitlab.com
  Hostname altssh.gitlab.com
  Port 443

Host github.com
  Hostname ssh.github.com
  Port 443

Host bitbucket.org
  Hostname altssh.bitbucket.org
  Port 443

# Default subnet for virbr0 (libvirt)
Host 192.168.122.*
  StrictHostKeyChecking no
  UserKnownHostsFile /dev/null

Host *.incus
  StrictHostKeyChecking no
  UserKnownHostsFile /dev/null

